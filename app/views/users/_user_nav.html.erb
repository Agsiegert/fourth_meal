<% if current_user %>
  <li>Logged in as <%= current_user.name %></li>
  <% if current_user.guest? %>
    <li><%= link_to "Become a member", sign_up_path %></li>
  <% else %>
    <li><%= link_to "Log Out", log_out_path %></li>
  <% end %>
<% else %>
  <li><%= link_to "Sign Up", sign_up_path %></li>
  <li><%= link_to "Log In", log_in_path %></li>
<% end %>
